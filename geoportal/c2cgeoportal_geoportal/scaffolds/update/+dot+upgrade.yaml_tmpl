default_project_file:
  include:
    - {{package}}/templates/.*
    - {{package}}/locale/.*
    - {{package}}/static/.*
    - {{package}}/static-ngeo/.*
    - {{package}}/jsbuild/.*
    - geoportal/{{package}}_geoportal/templates/.*
