    version: 2.3
  - from: setup.py
    to: geoportal/setup.py
    version: 2.3
  - from: testgeomapfish/models.py
    to: commons/testgeomapfish_commons/models.py
    version: 2.3
  - from: testgeomapfish
    to: geoportal/testgeomapfish_geoportal
