    - docker-compose.yaml.mako
    - setup.py
    - vars.yaml
    - Makefile
  exclude:
    - geoportal/testgeomapfish_geoportal/static-ngeo/components/README.md
    - mapserver/Dockerfile
    - mapserver/tinyows.xml.mako

files_to_move:
