---
checks:
  black: False
  isort: False
  codespell: False
  eof: False
  required_workflows: False
  dependabot_config: False

version:
  branch_to_version_re:
    - from: (prod-[0-9]+-[0-9]+)
      to: \1
    - from: (int-[0-9]+-[0-9]+)
      to: \1

publish:
  docker:
    images:
      - group: geoportal
        name: camptocamp/{{package}}-geoportal
      - group: config
        name: camptocamp/{{package}}-config
